import java.util.*;
class Character
{
public static void main(String[] args)
{
Scanner sc=new Scanner(System.in);
String str=obj.nextLine();
for(int i=0;i<str.length();i++)
{
char ch=str.charAt(i);
if(ch=='v')
{
System.out.print("a");
}
if(ch=='w')
{
System.out.print("b");
}
if(ch=='x')
{
System.out.print("c");
}
if(ch=='y')
{
System.out.print("d");
}
if(ch=='z')
{
System.out.print("e");
}
int b=(int)ch+5;
if(b>=88&b<=120)
{
System.out.print((char)b);
}
}
}
}
